{
    "name": "autogen/model",
    "description": "AutoGen Model Package - Intelligent Eloquent model generation from database tables",
    "type": "library",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "generator",
        "database",
        "autogen",
        "artisan",
        "code-generation"
    ],
    "homepage": "https://github.com/autogen/model",
    "license": "MIT",
    "authors": [
        {
            "name": "AutoGen Team",
            "email": "info@autogen.dev"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0",
        "doctrine/dbal": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "AutoGen\\Packages\\Model\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AutoGen\\Packages\\Model\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AutoGen\\Packages\\Model\\ModelPackageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    }
}